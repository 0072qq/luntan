<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.community.test.mapper.UserMapper">
    <insert id="insertUser" parameterType="user">
        insert into USER(USERNAME,ACCOUNT_ID,TOKEN,GMT_CREATE,GMT_MODIFIED) values (#{username},#{account_id},#{token},#{gmt_create},#{gmt_modified});
    </insert>

    <select id="findByToken" parameterType="String" resultType="user">
        select * from USER where TOKEN=#{token}
    </select>
</mapper>